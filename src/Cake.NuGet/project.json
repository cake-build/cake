{
  "version": "0.18.0-*",
  "dependencies": {
    "Cake.Core": {
      "target": "project"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "platform": "AnyCpu",
    "additionalArguments": [
      "/ruleset:../Cake.ruleset",
      "/additionalfile:../stylecop.json"
    ],
    "compile": {
      "includeFiles": [
        "../SolutionInfo.cs"
      ]
    }
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true
      }
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "debugType": "portable",
        "define": [
          "NETCORE"
        ]
      },
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "NuGet.Client": "3.5.0-beta2-1484"
      }
    },
    "net45": {
      "dependencies": {
        "NuGet.Core": "2.12.0"
      },
      "frameworkAssemblies": {}
    }
  }
}