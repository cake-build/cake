{
  "version": "0.15.3-*",
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../Test.ruleset",
      "/additionalfile:../stylecop.json"
    ]
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "warningsAsErrors": true
      }
    }
  },
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "Cake.Core": {
      "target": "project"
    },
    "Cake.Common": {
      "target": "project"
    },
    "Cake.Testing": {
      "target": "project"
    },
    "Cake.Testing.Xunit": {
      "target": "project"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "NSubstitute": "1.10.0.0"
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [
          "NETCORE"
        ]
      },
      "imports": [
        "dnxcore50"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "NSubstitute": "2.0.0-rc"
      }
    }
  },
  "testRunner": "xunit"
}